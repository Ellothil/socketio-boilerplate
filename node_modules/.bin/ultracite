#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/ultracite@5.4.4_@types+node_78f9f0fe75e94d0bc75b2f84e8f2860d/node_modules/ultracite/dist/node_modules:/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/ultracite@5.4.4_@types+node_78f9f0fe75e94d0bc75b2f84e8f2860d/node_modules/ultracite/node_modules:/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/ultracite@5.4.4_@types+node_78f9f0fe75e94d0bc75b2f84e8f2860d/node_modules:/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/ultracite@5.4.4_@types+node_78f9f0fe75e94d0bc75b2f84e8f2860d/node_modules/ultracite/dist/node_modules:/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/ultracite@5.4.4_@types+node_78f9f0fe75e94d0bc75b2f84e8f2860d/node_modules/ultracite/node_modules:/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/ultracite@5.4.4_@types+node_78f9f0fe75e94d0bc75b2f84e8f2860d/node_modules:/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ultracite/dist/index.js" "$@"
else
  exec node  "$basedir/../ultracite/dist/index.js" "$@"
fi
