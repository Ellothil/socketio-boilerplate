#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_0f0ff852caf0e0c53c6dfdd18b7053fd/node_modules/ts-jest/node_modules:/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_0f0ff852caf0e0c53c6dfdd18b7053fd/node_modules:/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_0f0ff852caf0e0c53c6dfdd18b7053fd/node_modules/ts-jest/node_modules:/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@_0f0ff852caf0e0c53c6dfdd18b7053fd/node_modules:/proc/cygdrive/e/Coding/socketio-boilerplate/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
